{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "repoBaseUrl": {
      "type": "string",
      "metadata": {
        "description": "Base URL of the repository"
      }
    },
    "sqlServerName": {
      "type": "string",
      "metadata": {
        "description": "The name of the SQL Server"
      }
    }
  },
  "variables": {},
  "resources": [
    {
      "type": "Microsoft.Resources/deployments",
      "name": "sqlServerTemplate",
      "apiVersion": "2017-05-10",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[concat(parameters('repoBaseUrl'), '/server/server.template.json')]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "sqlServerName": {
            "value": "[parameters('sqlServerName')]"
          }
        }
      }
    },
    {
      "type": "Microsoft.Resources/deployments",
      "name": "sqlEdFiDatabasesTemplate",
      "apiVersion": "2017-05-10",
      "dependsOn": [
        "[resourceId('Microsoft.Resources/deployments', 'sqlServerTemplate')]"
      ],
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[concat(parameters('repoBaseUrl'), '/service/service-subsequent.template.json')]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
        }
      }
    }
  ],
  "outputs": {
  }
}