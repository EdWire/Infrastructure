{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "repoBaseUrl": {
      "type": "string"
    },
    "apimServiceName": {
      "type": "string"
    },
    "apimProxyHostName": {
      "type": "string"
    }
  },
  "variables": {
    "apimServiceUrl": "[concat('https://', parameters('apimProxyHostName'))]"
  },
  "resources": [
    {
      "type": "Microsoft.ApiManagement/service/apis",
      "name": "[concat(parameters('apimServiceName'), '/edfi-api-v3-2')]",
      "apiVersion": "2019-01-01",
      "properties": {
        "displayName": "Ed-Fi Api v3.2",
        "apiRevision": "1",
        "description": "Ed-Fi Api v3.2",
        "serviceUrl": "[variables('apimServiceUrl')]",
        "path": "edfi",
        "protocols": [
          "https"
        ],
        "apiVersion": "v3.2",
        "apiVersionSetId": "[concat(resourceId('Microsoft.ApiManagement/service', parameters('apimServiceName')), '/api-version-sets/versionset-edfi-api')]",
        "format": "swagger-link-json",
        "value": "https://eginternaldevwestus.blob.core.windows.net/packages/EdGraph.EdFi.Ods.App/Core.3.2.0.0.20191021.1/edfi-core-oauth-descriptors-resources-enrollment.json?st=2019-10-25T21%3A04%3A53Z&se=2019-10-26T21%3A04%3A53Z&sp=rl&sv=2018-03-28&sr=b&sig=5PnxosL%2BTLITjOYYxa5BRRAdHLljgy%2FYtHgOIlkCQCs%3D"
      }
    },
    {
      "type": "Microsoft.ApiManagement/service/products/apis",
      "name": "[concat(parameters('apimServiceName'), '/starter/edfi-api-v3-2')]",
      "apiVersion": "2019-01-01",
      "properties": {},
      "dependsOn": [
        "[resourceId('Microsoft.ApiManagement/service/apis', parameters('apimServiceName'), 'edfi-api-v3-2')]"
      ]
    },
    {
      "type": "Microsoft.ApiManagement/service/products/apis",
      "name": "[concat(parameters('apimServiceName'), '/unlimited/edfi-api-v3-2')]",
      "apiVersion": "2019-01-01",
      "properties": {},
      "dependsOn": [
        "[resourceId('Microsoft.ApiManagement/service/apis', parameters('apimServiceName'), 'edfi-api-v3-2')]"
      ]
    },
    {
      "type": "Microsoft.ApiManagement/service/apis/policies",
      "name": "[concat(parameters('apimServiceName'), '/edfi-api-v3-2/policy')]",
      "apiVersion": "2019-01-01",
      "properties": {
        "value": "[concat(parameters('repoBaseUrl'), '/apis/edfi/v3.2/policy-edfi.v3.2.xml')]",
        "format": "rawxml-link"
      },
      "dependsOn": [
        "[resourceId('Microsoft.ApiManagement/service/apis', parameters('apimServiceName'), 'edfi-api-v3-2')]"
      ]
    }
  ]
}