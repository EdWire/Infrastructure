{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "keyvaultName": {
      "type": "string",
      "metadata": {
        "description": "The name of the Key Vault"
      }
    },
    "sqlAdminUsername": {
      "type": "string",
      "metadata": {
        "description": "Username used to login to SQL Server (SQL Authentication)"
      }
    },
    "sqlAdminPassword": {
      "type": "string",
      "metadata": {
        "description": "Password used to login to SQL Server (SQL Authentication)"
      }
    }
  },
  "resources": [
    {
      "type": "Microsoft.KeyVault/vaults/secrets",
      "name": "[concat(parameters('keyvaultName'), '/', 'SQL--Admin--Username')]",
      "apiVersion": "2018-02-14",
      "dependsOn": [],
      "properties": {
        "value": "[parameters('sqlAdminUsername')]"
      }
    },
    {
      "type": "Microsoft.KeyVault/vaults/secrets",
      "name": "[concat(parameters('keyvaultName'), '/', 'SQL--Admin--Password')]",
      "apiVersion": "2018-02-14",
      "dependsOn": [],
      "properties": {
        "value": "[parameters('sqlAdminPassword')]"
      }
    }
  ]
}
